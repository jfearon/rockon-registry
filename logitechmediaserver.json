{
    "Logitech Media Server": {
        "containers": {
            "logitechmediaserver": {
                "image": "larsks/logitech-media-server",
                "launch_order": 1,
                "ports": {
                    "9000": {
                        "description": "Webserver Port. Suggested default: 9000.",
                        "host_default": 9000,
                        "label": "Webserver port",
                        "protocol": "tcp",
                        "ui": true
                    },
                    "9090": {
                        "description": "CLI Port. Suggested default: 9090.",
                        "host_default": 9090,
                        "label": "CLI port",
                        "protocol": "tcp"
                    },
                    "3483": {
                        "description": "SlimProto Port. Suggested default: 3483. Should NOT be changed",
                        "host_default": 3483,
                        "label": "Slimproto port"
                    }

                },
                "volumes": {
                    "/srv/music": {
                        "description": "Select the share containing your media",
                        "label": "Media Storage",
                        "min_size": 1073741824
                    },
                    "/srv/squeezebox": {
                        "description": "Select the share to store media server config",
                        "label": "Media Server Config",
                        "min_size": 1073741824
                    }
                }
            }
        },
        "description": "Logitech Media Server",
        "ui": {
            "slug": ""
        },
        "volume_add_support": true,
        "website": "http://mysqueezebox.com",
        "version": "1.0"
    }
}
